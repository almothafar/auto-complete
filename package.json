    "@typescript-eslint/parser": "^8.5.0",
    "copyfiles": "^2.4.1",
    "cypress": "latest",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.5.0",
    "jasmine-core": "~5.3.0",
